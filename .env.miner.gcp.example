# Bittensor Configuration
NETUID=234
SUBTENSOR_NETWORK="test"
SUBTENSOR_CHAIN_ENDPOINT="wss://test.finney.opentensor.ai:443"
WALLET_NAME="start"
HOTKEY="test-miner2"
AXON_PORT="22181"

# Cloud Provider Selection
PROVIDER=GCP

# Generic Cloud Credentials
CLOUD_PROJECT_ID=projectname
CLOUD_AUTH_ID=projectname-vm-manager@dprojectname.iam.gserviceaccount.com
CLOUD_AUTH_SECRET="-----BEGIN PRIVATE KEY----- /n -----END PRIVATE KEY-----"
CLOUD_RESOURCE_GROUP=  # Not used by GCP, leave empty

# Generic Network Configuration
VPC_NAME=projectname-vpc
SUBNET_NAME=projectname-vpc-24

# Generic Compute Configuration
REGION=europe-central2-a
VM_SIZE_SMALL=e2-medium        # For King (can be different/larger)
VM_SIZE_LARGE=n2-standard-4    # For Traffic Generators
NUM_TGENS=2

# Optional: Custom VM Specifications for King
# CUSTOM_KING_CPU_COUNT=2
# CUSTOM_KING_RAM_MB=4096

# Optional: Custom VM Specifications for TGens
# CUSTOM_TGEN_CPU_COUNT=8
# CUSTOM_TGEN_RAM_MB=16384